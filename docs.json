{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Tellscope",
  "colors": {
    "primary": "#212AA5",
    "light": "#9EC2FF",
    "dark": "#03018C"
  },
  "background": {
    "light": "#FFFFFE",
    "dark": "#000000"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["index", "quickstart"]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/introduction",
              "api-reference/authentication"
            ]
          },
          {
            "group": "Feedback",
            "pages": [
              "api-reference/feedback/list",
              "api-reference/feedback/get",
              "api-reference/feedback/create",
              "api-reference/feedback/delete"
            ]
          },
          {
            "group": "Contacts",
            "pages": [
              "api-reference/contacts/list",
              "api-reference/contacts/get"
            ]
          },
          {
            "group": "Accounts",
            "pages": [
              "api-reference/accounts/list",
              "api-reference/accounts/get"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://app.tellscope.io",
          "icon": "gauge"
        },
        {
          "anchor": "Support",
          "href": "mailto:support@tellscope.io",
          "icon": "envelope"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@tellscope.io"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://app.tellscope.io"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/tellscope",
      "linkedin": "https://linkedin.com/company/tellscope"
    }
  }
}
